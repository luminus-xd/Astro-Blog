{
    "extends": "astro/tsconfigs/strict",
    "compilerOptions": {
        "baseUrl": ".",
        "strictNullChecks": true,
        "paths": {
            "@components/*": [
                "src/components/*"
            ],
            "@layouts/*": [
                "src/layouts/*"
            ],
            "@library/*": [
                "src/library/*"
            ],
            "@pages/*": [
                "src/pages/*"
            ],
            "@hooks/*": [
                "src/hooks/*"
            ],
            "@styles/*": [
                "src/styles/*"
            ],
            "@consts": [
                "src/consts.ts"
            ]
        }
    },
}